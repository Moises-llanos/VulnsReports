{
  "name": "vulnsreport",
  "displayName": "Vulnsreport",
  "description": "This extension was developed to keep an eye on the reported vulnerabilities. This extension will be integrated into our workspace to speed up and detect new vulnerabilities in a timely manner.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    
  ],
  "main": "./out/extension.js",
  "contributes": {
    
    "commands": [
      {
        "command": "vulnsreport.run",
        "title": "Generar reporte de vulnerabilidades",
        "category": "vulnsreport"
      }
    ],
    "webviews": [
      {
        "viewType": "webview",
        "icon": {
          "light": "./assets/vulns.svg",
          "dark": "./assets/vulns.svg"
        }
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
          "Vulnsreport.token": {
              "type": "string",
              "default": "",
              "description": "Token de seguridad para vulnsreport."
          } 
      }
  }
  },
  "extensionDependencies": [
    "vscode.git"
],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
